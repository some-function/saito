/* Game Advanced Options Styling*/

.game-wizard-options-toggle {
	padding-top: 1rem;
}

.game-wizard-options-toggle-text {
	border-bottom: 1px dashed;
	cursor: pointer;
}

.game-wizard-invite {
	width: 100%;
	min-height: 7rem;
	display: block !important;
