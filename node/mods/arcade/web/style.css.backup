}

.game-wizard-other-btns {
	display: none;
	/* display: flex; */
	flex-direction: row;
	justify-content: space-between;
}

.arcade-wizard-overlay {
	display: grid;
	max-height: 95vh;
	height: min-content;
	width: min(900px, 90vw);
	grid-template-rows: min-content auto;
	background: var(--saito-arcade-overlay-background);
	padding: 2rem;
	border-radius: inherit;
	color: var(--color-white);
	border: 1px solid var(--saito-arcade-overlay-background);
}

.arcade-wizard-overlay .arcade-wizard-game-container {
	min-height: 17rem;
	display: grid;
	grid-template-columns: 3fr 7fr;
	grid-gap: 1.5rem;
}

.arcade-wizard-overlay .arcade-wizard-game-image {
	background-position: center;
	background-repeat: no-repeat;
	background-size: cover;
}

.arcade-wizard-overlay .arcade-wizard-game-image img {
	width: 100%;
	object-fit: cover;
	height: 100%;
}

.arcade-wizard-game-name {
	font-size: 3.5rem;
	margin-bottom: 1rem;
}

.arcade-wizard-game-description {
	font-size: 2rem;
	line-height: 3rem;
}

.arcade-wizard-game-controls {
	display: grid;
	grid-template-columns: 3fr 7fr;
	grid-gap: 1rem;
	margin-top: 2rem;
}

.arcade-wizard-overlay select {
	background: transparent;
	border: 1px solid var(--saito-primary);
	color: var(--color-white);
	font-size: 2rem;
	appearance: none;
	padding: 0 1rem;
	border-radius: 0 1rem 1rem 0;
}

.arcade-wizard-overlay select:focus {
	outline: none;
}

.saito-select::after {
	border-left: 1px solid var(--saito-primary);
}

.arcade-wizard-game-controls .settings > div.overlay-input {
	position: relative;
	display: flex;
	width: 20em;
	height: 3em;
	line-height: 3;
	background-color: transparent;
	overflow: hidden;
	width: 100%;
	outline: none;
	border: none;
	background-color: none;
}

.arcade-wizard-game-controls .settings {
	display: flex;
	flex-direction: column;
	row-gap: 1rem;
}

.arcade-wizard-game-controls .settings > div.overlay-input::after {
	content: '\25BC';
	position: absolute;
	top: 0;
	right: 0;
	padding: 0 1em;
	color: var(--color-white);
	background: var(--background-color-button-mod-arcade);
	cursor: pointer;
