.arcade-game-overlay {
	background: var(--saito-arcade-background); /* testing original */
	width: 60vw;
	max-width: 100rem;
	min-width: min(66rem, 95vw);
	height: max-content;
	max-height: 90vh;
	padding: 2rem;
	border-radius: 0.5rem;
	display: flex;
	flex-flow: column;
	grid-gap: 2rem;
}

.arcade-game-players {
	display: grid;
	column-gap: 2rem;
	row-gap: 1rem;
	grid-template-columns: 1fr 1fr;
}

.arcade-game-playerbox {
	display: grid;
	grid-template-columns: 5rem 1fr;
	grid-gap: 0;
	column-gap: 1.5rem;
	padding: 1rem 1rem 1rem;
	transition: all 0.2s;
	background-color: var(--background-color-button-modal); 
	position: relative;
}

.arcade-game-playerbox.available_slot {
	cursor: pointer;
}

.debug_overlay {
	width: min(800px, 90vw);
	max-height: 100vh;
	overflow: scroll;
	padding: 2rem;
}

.arcade-game-playerbox.requested_player {
	opacity: 0.6;
	filter: grayscale(0.7);
}

.arcade-game-overlay .saito-table-row.arcade-game-player-row {
	display: grid;
	grid-template-columns: min-content 1fr min-content 1fr;
	grid-gap: 1rem;
}

.arcade-game-overlay-header-image {
	background-size: cover;
	width: 11rem;
	height: 7rem;
	float: left;
	margin-right: 1rem;
}

.arcade-game-overlay-header-title-box-title {
	font-size: 4rem;
}

.arcade-game-overlay-header-title-box-desc {
	font-size: 2rem;
}

.arcade-game-overlay-body {
	display: grid;
	row-gap: 1rem;
	grid-template-columns: 1fr;
}

.arcade-game-overlay .saito-table-row {
	grid-template-columns: 4rem 1fr;
	align-items: center;
	cursor: unset;
}

.arcade-overlay-game-options .saito-table-row {
	grid-template-columns: minmax(8rem, auto) 1fr;
	grid-column-gap: min(10rem, 12vw);
}

.arcade-game-options-key {
	font-weight: bold;
}

.arcade-game-options {
	overflow-y: auto;
	box-shadow: var(--color-shadow-original);
	padding: 2rem;
	border-radius: 0.5rem;
	display: flex;
	flex-direction: column;
	row-gap: 2rem;
	max-height: 60vh;
	overflow-y: auto;
}

.arcade-game-controls {
	display: flex;
	flex-direction: row;
	column-gap: 1rem;
	flex-flow: row-reverse;
}

.arcade-game-controls .saito-button {
	margin: 0;
	width: max-content;
	min-width: 20rem;
	height: 7rem;
	font-size: 2rem;
	line-height: 5rem;
}

.saito-identicon-box.empty-slot {
	width: 3.5rem;
	border: 1px solid var(--border-color-general-dividers);
	box-shadow: -1px -1px #000;
	background-color: var(--background-color-shim-normal);
	height: 3.5rem;
	display: flex;
	align-items: center;
	justify-content: center;
}

.arcade_game_overlay_loader {
	width: min(75rem, 100vw);
	background: var(--saito-arcade-background);
	padding: 10rem;
}

.arcade_game_overlay_loader .arcade-game-initializer-success-title,
.arcade_game_overlay_loader .game-loader-title {
	font-size: 2.5rem;
	text-align: center;
	margin: 0;
}

.arcade_game_overlay_loader .arcade-game-initializer-success-button {
	font-size: 1.8rem;
	width: 70%;
	margin: 0;
}

.arcade_game_overlay_loader .arcade-initializer {
	text-align: center;
	min-width: 18rem;
	display: flex;
	flex-direction: column;
	justify-content: space-around;
	gap: 5rem;
	align-items: center;
}

.arcade-game-playerbox .winner::before {
	content: 'â™•';
	display: inline-block;
	font-style: normal;
	font-variant: normal;
	text-rendering: auto;
	position: absolute;
	top: -1.1rem;
	left: -1rem;
	font-size: 30px;
	font-weight: 900;
	color: var(--saito-red);
	text-shadow: 0px 0px 3px var(--color-white);
}

.arcade-game-playerbox.online .online-status-indicator:after {
	top: 3.5rem;
	left: 3.5rem;
}

@media screen and (max-width: 1000px) {
	.arcade-game-overlay {
		width: 90vw;
		min-width: 60rem;
	}
	.arcade-game-controls .saito-button {
		min-width: 18rem;
	}
}

@media screen and (max-width: 615px) {
	.arcade-game-overlay {
		width: 95vw;
		min-width: unset;
		display: grid;
		grid-template-rows: min-content 1fr min-content;
		grid-template-columns: minmax(100px, 1fr);
	}

	.arcade-game-overlay-body {
		min-height: 0;
		max-height: 100%;
	}

	.arcade-game-controls {
		display: grid;
		grid-gap: 1rem;
		grid-template-areas: "a a" "b c";
		grid-template-columns: 1fr 1fr;
		width: 100%;
	}
	.arcade-game-controls .saito-button {
		width: 100%;
		height: 5rem;
		font-size: 2rem;
		line-height: 3rem;
		min-width: unset;
	}

	.arcade-game-controls .saito-button:first-child {
		grid-column: span 2;
	}

	.arcade-game-options{
		padding: unset;
	}
	
	.arcade-game-players {
		display: flex;
		flex-direction: column;
		row-gap: 1rem;
	}
}

@media screen and (max-width: 500px) {
	.arcade-game-overlay-header {
		display: block;
	}
	.arcade-game-overlay-header-image {
		display: none;
	}
}

@media screen and (max-height: 550px) {
	.arcade-game-overlay {
		max-height: 98vh;
		grid-gap: 1rem;
		padding: 1rem;
	}

	.arcade-game-controls .saito-button {
		height: 6rem;
		line-height: 4rem;
	}
}

