  border-left: 4px solid var(--saito-border-color);
  /* margin: 1em 0; */
  padding-left: 1em;
  color: var(--saito-font-color);
  font-style: italic;
}

.richtext-preview blockquote > blockquote {
  margin-left: 0;
}

/* Code blocks */
.richtext-preview pre {
  background: var(--saito-background-color-alt);
  padding: 1em;
  border-radius: 4px;
  overflow-x: auto;
  /* margin: 1em 0; */
}

.richtext-preview code {
  font-family: monospace;
  padding: 0.2em 0.4em;
  background: var(--saito-background-color-alt);
  border-radius: 3px;
  font-size: 0.9em;
}

.richtext-preview pre > code {
  padding: 0;
  background: transparent;
  font-size: 0.9em;
  white-space: pre;
}

/* Links */
.richtext-preview a {
  color: var(--saito-primary);
  text-decoration: none;
}

.richtext-preview a:hover {
  text-decoration: underline;
}

/* Images */
.richtext-preview img {
  max-width: 100%;
  /* height: auto; */
  /* margin: 1em 0; */
}

/* Horizontal rule */
.richtext-preview hr {
  border: 0;
  border-top: 1px solid var(--saito-border-color);
  /* margin: 2em 0; */
}

/* Inline text styling */
.richtext-preview strong {
  font-weight: 600;
}

.richtext-preview em {
  font-style: italic;
}

.richtext-preview del {
  text-decoration: line-through;
}

/* Paragraphs */
.richtext-preview p {
  /* margin: 1em 0; */
}

@media (max-width: 768px) {
  .richtext-preview {
    padding-left: 1.5rem;
    height: auto;
    min-height: 40rem;
  }

  .richtext-preview pre {
    padding: 0.75em;
  }
}

@media (max-width: 480px) {
  .richtext-preview {
    padding-left: 1rem;
  }
}
html,
body {
  height: 100%;
  overflow-y: auto;
  margin: 0;
  padding: 0;
  -ms-overflow-style: none;
  scrollbar-width: none;
  scroll-behavior: smooth;
}

/* .html::-webkit-scrollbar body::-webkit-scrollbar {
  display: initial;
} */

.scroll-button,
.saito-floating-plus-btn {
  background-color: var(--saito-primary);
  width: 6rem;
  height: 6rem;
  border-radius: 50%;
  display: none; /*flex;*/
  align-items: center;
  justify-content: center;
  cursor: pointer;
  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.2);
  z-index: 1000;
  transition: background-color 0.3s;
}

.saito-floating-container {
  gap: 1rem;
  display: flex;
}

.saito-user {
  display: grid;
  column-gap: 1rem;
  grid-template-columns: min-content 1fr;
  grid-template-rows: min-content 1fr;
  justify-items: start;
  width: 100%;
  white-space: nowrap;
  row-gap: 0;
}

.saito-user.single-line {
  display: flex;
  align-items: center;
  font-size: 1.4rem;
}

.saito-user.single-line .saito-address {
  color: var(--font-color-primary);
}

.saito-user.single-line .saito-userline {
  margin-left: auto;
  font-size: 1.2rem;
}

.saito-blog-layout {
  max-height: 100vh;
  position: absolute;
  height: calc(100% - var(--saito-header-height));
  width: 100vw;
  top: var(--saito-header-height);
  padding: 0 calc((100vw - var(--saito-width)) / 2);
  display: grid;
  grid-template-columns: 256px 1fr;
  column-gap: 1rem;
}

.saito-blog-layout.blog-view {
  grid-template-columns: 1fr;
}

/* Left Column */
.left-column {
  width: 100%;
  border-right: 1px solid var(--saito-border-color);
  padding: 2rem;
  padding-top: 3.5rem; /* to align with blog list */
  box-shadow: 1px 0 2px rgba(0, 0, 0, 0.05);
  position: sticky;
  height: 100%;
  display: flex;
  flex-direction: column;
  gap: 2rem;
}

.blog-view > .left-column {
  display: none;
}

.filter-label {
  display: block;
  font-weight: 500;
  font-size: 1.2rem;
  margin-bottom: 8px;
}

.filter-container select {
  width: 100%;
  padding: 1rem;
  border: 1px solid var(--saito-border-color);
  border-radius: 0.5rem;
  box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05);
}

.filter-select {
  width: 100%;
  padding: 8px;
  border: 1px solid var(--saito-border-color);
  border-radius: 0.5rem;
  box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05);
}

.filter-select:focus {
  outline: none;
  border-color: var(--saito-border-color);
  box-shadow: 0 0 0 2px rgba(59, 130, 246, 0.5);
}

/* Center Column */
.center-column {
  flex: 1;
  padding: 24px;
  max-width: 768px;
  height: 100%;
  width: 100%;
  -ms-overflow-style: none;
  /* IE and Edge */
  scrollbar-width: none;
  /* Firefox */
  overflow-y: scroll;
}

.blog-view .center-column {
  margin: 0 auto;
}

/* .center-column::-webkit-scrollbar {
  display: initial;
} */

.user-header {
  margin-bottom: 2rem;
  width: 100%;
}

.user-header-content {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-left: 2rem;
}

.user-header .saito-user {
  margin-bottom: 0;
}

/* Post List */
.posts-list {
  display: flex;
  flex-direction: column;
  min-height: min-content;
}

.post-card {
  border-radius: 0.5rem;
  padding: 0.5rem;
  padding: 1.5rem;
  cursor: pointer;
  box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05);
  transition: box-shadow 0.2s ease;
  max-width: 80rem;
}

.post-card-title {
  font-weight: 900;
  font-size: 2.3rem;
  margin-bottom: 1rem; /* Consistent margin below title */
  overflow: hidden;
  width: 55rem;
  display: -webkit-box;
}

.post-card-preview {
  margin-bottom: 16px;
  display: -webkit-box;
  overflow: hidden;
}

.new-post-container {
  margin-bottom: 1.5rem;
}

.new-post-container > button {
  width: 100%;
}

.post-card:hover {
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
}

.post-card-content {
  display: flex;
  gap: 1.5rem;
  height: 8.5rem;
  align-items: center;
}

.post-card-main {
  flex: 1;
  min-width: 0;
  display: flex;
  flex-direction: column;
  justify-self: center;
}

.read-more-button {
  display: flex;
  align-items: center;
  gap: 0.5rem;
  border: none;
  border-radius: 0.375rem;
  font-weight: 500;
  cursor: pointer;
  transition: all 0.2s ease;
}

.post-card-meta {
  display: flex;
  justify-content: space-between;
  position: relative;
  align-items: flex-end;
}

.post-card-meta .engagement-stat {
  position: absolute;
}
.post-published-div {
  margin-bottom: 1rem;
}

.post-card-preview {
  display: -webkit-box;
  overflow: hidden;
  line-height: 1.5;
}

.post-card-preview p {
  font-size: 1.8rem;
}

.post-card-image {
  flex-shrink: 0;
  width: 130px;
  height: 100px;
  border-radius: 0.375rem;
  overflow: hidden;
  height: 100%;
}

.end-message {
  margin-top: 5rem;
  text-align: center;
}

.engagement-button {
  background: none;
  border: none;
  cursor: pointer;
  padding: 0;
  transition: color 0.2s ease;
}

.engagement-button,
.engagement-stat {
  display: flex;
  align-items: center;
  gap: 8px;

  right: 1rem;
}

.icon {
  width: 16px;
  height: 16px;
}

/* No post */
.no-posts-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  min-height: 60vh;
  padding: 20px;
  text-align: center;
}

.icon-circle {
  background-color: var(--saito-primary);
  border-radius: 50%;
  padding: 2.4rem;
  margin-bottom: 2.4rem;
}

.no-posts-title {
  font-size: 2.4rem;
  margin-bottom: 1.2rem;
}

.no-posts-message {
  margin-bottom: 2.4rem;
  max-width: 44rem;
}

.create-post-button {
  background-color: var(--saito-secondary);
  padding: 12px 24px;
  border: none;
  border-radius: 6px;
  cursor: pointer;
}

.preview-image {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

@media (max-width: 768px) {
  .post-card-image {
    width: 100%;
    height: 200px;
    order: -1;
  }

  .post-card-main {
    width: 100%;
  }

  .saito-blog-layout {
    grid-template-columns: 1fr;
  }

  .left-column {
    display: none;
  }
}

/* Responsive Breakpoints */
@media (max-width: 1024px) {
  .post-card-title {
    width: 100%;
    font-size: 2rem;
  }
}

@media (max-width: 768px) {
  .post-card-content {
    flex-direction: column;
    height: auto;
    gap: 1rem;
  }

  .post-card-image {
    width: 100%;
    height: 200px;
    order: -1;
  }

  .post-card-main {
    width: 100%;
  }

  .post-card-title {
    font-size: 1.8rem;
  }
}

@media (max-width: 768px) {
  .post-card-image {
    width: 100%;
    height: 150px;
  }

  .left-column {
    display: none;
  }

  .post-card-content {
    flex-direction: column;
    height: auto;
    gap: 1rem;
  }

  .saito-floating-plus-btn {
    display: flex;
  }
}

@media (max-width: 480px) {
  .saito-blog-layout {
    padding: 0;
  }

  .post-card {
    padding: 1rem;
  }

  .post-card-image {
    height: 150px;
  }

  .post-card-title {
    font-size: 1.6rem;
  }

  .saito-user.single-line .saito-userline {
    margin-left: 0;
    margin-top: 0.5rem;
  }
}

@media (max-width: 480px) {
  .post-card-image {
    width: 100%;
    height: 120px;
  }
}
