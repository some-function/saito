.richtext-preview ol {
  padding-left: 2em;
  /* margin: 1em 0; */
}

.richtext-preview li {
  /* margin: 0.5em 0; */
}

.richtext-preview li > ul,
.richtext-preview li > ol {
  /* margin: 0.5em 0; */
}

/* Blockquotes */
