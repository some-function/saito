.richtext-content {
  border: none !important;
  padding-left: 0;
}

.richtext-content img:not(:first-of-type) {
  display: block;
  max-width: 100%;
  height: auto;
}

.richtext-content img[alt='HIDDEN_IMAGE'] {
  display: none !important;
